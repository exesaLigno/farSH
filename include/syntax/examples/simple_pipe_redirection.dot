digraph G {
    dummy [shape=none; fontname="Courier"; label="cat $USERNAME-log.txt | grep error 'critical warning' >> errors.txt"]
    
    pipe_redirection [shape=record]

    program1 [shape=record; label="invocation"]
    word1 [shape=record; label="{word | \"cat\"}"]
    composition1 [shape=record; label="composition"]
    envref1 [shape=record; label="environment_variable_reference"]
