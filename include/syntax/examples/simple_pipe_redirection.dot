digraph G {
    dummy [shape=none; fontname="Courier"; label="cat $USERNAME-log.txt | grep error 'critical warning' >> errors.txt"]
    
    command [shape=record]
    pipe_redirection [shape=record]

    program1 [shape=record; label="invocation"]
    word1 [shape=record; label="{word | cat}"]
    argument1 [shape=record; label="argument"]
    composition1 [shape=record; label="composition"]
    envref1 [shape=record; label="environment_variable_reference"]
    word2 [shape=record; label="{word | USERNAME}"]
    word3 [shape=record; label="{word | -log.txt}"]

    program2 [shape=record; label="invocation"]
    word4 [shape=record; label="{word | grep}"]
    argument2 [shape=record; label="argument"]
    word5 [shape=record; label="{word | error}"]
    argument3 [shape=record; label="argument"]
    rawstring [shape=record; label="{raw_string_literal | critical warning}"]

    file_redirection [shape=record]
    argument4 [shape=record; label="argument"]
    word6 [shape=record; label="{word | errors.txt}"]
    
    dummy->command [style="dotted", arrowhead="none"]
    
    command -> pipe_redirection [label="execution_unit"]
    pipe_redirection -> program1 [label="source"]
    program1 -> word1 [label="program_name"]
    program1 -> argument1 [label="argument"]
    argument1 -> composition1 [label="value"]
    composition1 -> envref1
    envref1 -> word2 [label="variable_name"]
    composition1 -> word3

    pipe_redirection -> file_redirection [label="destination"]
    file_redirection -> program2 [label="source"]
    program2 -> word4 [label="program_name"]
    program2 -> argument2 [label="argument"]
    argument2 -> word5 [label="value"]
    program2 -> argument3 [label="argument"]
    argument3 -> rawstring [label="value"]
    file_redirection -> argument4 [label="destination"]
    argument4 -> word6 [label="value"]
    // command -> pipe_redirection
    // pipe_redirection -> program1 [label="source"]
    // pipe_redirection -> file_redirection [label="destination"]
    // file_redirection -> program2 [label="source"]
    // file_redirection -> argument4 [label="destination"]
    // program1 -> argument1
    // program2 -> argument2
    // program2 -> argument3
}
